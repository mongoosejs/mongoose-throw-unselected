{
  "name": "mongoose-throw-unselected",
  "version": "0.1.2",
  "author": "Valeri Karpov <val@karpov.io>",
  "description": "Throws an error whenever you access a field on a mongoose document that is projected out",
  "devDependencies": {
    "acquit": "1.0.2",
    "acquit-ignore": "0.1.0",
    "acquit-markdown": "0.1.0",
    "istanbul": "0.4.2",
    "mocha": "5.x",
    "mongodb": "3.x",
    "mongoose": "5.x"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "license": "Apache 2.0",
  "main": "index.js",
  "peerDependencies": {
    "mongoose": ">= 4.12.0 || 5.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vkarpov15/mongoose-throw-unselected.git"
  },
  "scripts": {
    "coverage": "istanbul cover _mocha -- -R spec ./test/*",
    "docs": "acquit-markdown -r acquit-ignore -p './test/examples.test.js' -h './HEADER.md' > README.md",
    "test": "mocha ./test/*"
  }
}
